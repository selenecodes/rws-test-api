{
  "name": "test",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "private": true,
  "description": "Rijkswaterstaat API",
  "scripts": {
    "start": "node dist/server",
    "clean": "rimraf dist",
    "dev": "ts-node-dev --no-notify --respawn --transpile-only src/server",
    "build": "npm -s run clean && npm -s run generate && tsc",
    "generate": "pal g && npm -s run generate:prisma && npm -s run generate:nexus",
    "generate:prisma": "prisma generate",
    "generate:nexus": "ts-node --transpile-only src/nexusSchema",
    "db:dev": "prisma migrate dev",
    "db:seed": "prisma migrate reset -f"
  },
  "prisma": {
    "seed": "ts-node prisma/seed.ts"
  },
  "dependencies": {
    "@paljs/nexus": "4.1.0",
    "@prisma/client": "^3.5.0",
    "apollo-server": "^3.5.0",
    "graphql": "15.8.0",
    "nexus": "1.3.0"
  },
  "devDependencies": {
    "@faker-js/faker": "^7.3.0",
    "@paljs/cli": "4.1.0",
    "@types/node": "15.14.9",
    "prettier": "2.7.1",
    "prisma": "^3.5.0",
    "rimraf": "^3.0.2",
    "ts-node": "10.9.1",
    "ts-node-dev": "1.1.8",
    "typescript": "4.7.4"
  },
  "prettier": {
    "singleQuote": true,
    "semi": false,
    "trailingComma": "all"
  },
  "repository": "",
  "author": "Selene Blok"
}
